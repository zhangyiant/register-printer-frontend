{
  "name": "register-printer-frontend",
  "version": "0.2.11",
  "author": "antee",
  "description": "A UI tool to generate Register related codes.",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxyConfig=proxy.config.json",
    "build": "ng build --base-href=/register-printer --deploy-url=/register-printer/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "ng build --base-href=. && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.9",
    "@angular/compiler": "^10.0.9",
    "@angular/core": "^10.0.9",
    "@angular/forms": "^10.0.9",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.9",
    "@angular/platform-browser-dynamic": "^10.0.9",
    "@angular/router": "^10.0.9",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.1.1",
    "electron-updater": "^4.2.5",
    "lodash": "^4.17.20",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular-guru/electron-builder": "^8.0.0",
    "@angular/cli": "^10.0.6",
    "@angular/compiler-cli": "^10.0.9",
    "@angular/language-service": "^10.0.9",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.161",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^9.2.0",
    "electron-builder": "^22.4.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.0"
  },
  "build": {
    "appId": "com.anteestudio.registerprinter",
    "asar": true,
    "asarUnpack": [
      "app/RegisterPrinter.exe"
    ],
    "files": [
      "index.js",
      "dist/register-printer-frontend/**/*",
      "app/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "registerPrinter.ico"
    },
    "publish": {
      "provider": "generic",
      "url": "http://zhangyiant.oicp.net:9000"
    }
  }
}
