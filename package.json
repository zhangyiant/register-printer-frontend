{
  "name": "register-printer-frontend",
  "version": "0.4.8",
  "author": "antee",
  "description": "A UI tool to generate Register related codes.",
  "main": "./dist/electron/index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxyConfig=proxy.config.json",
    "build": "ng build --base-href=.",
    "test2": "ng test",
    "test": "npm run test -w packages/register-printer",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "npm run build -w packages/register-printer && tsc --build tsconfig.electron.json && webpack --config webpack.config.preload.js && ng build --base-href=. && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.5",
    "@angular/cdk": "^12.2.5",
    "@angular/common": "^12.2.5",
    "@angular/compiler": "^12.2.5",
    "@angular/core": "^12.2.5",
    "@angular/forms": "^12.2.5",
    "@angular/material": "^12.2.5",
    "@angular/platform-browser": "^12.2.5",
    "@angular/platform-browser-dynamic": "^12.2.5",
    "@angular/router": "^12.2.5",
    "angular-split": "^5.0.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.1.1",
    "electron-updater": "^4.2.5",
    "lodash": "^4.17.20",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.5",
    "@angular/cli": "^12.2.5",
    "@angular/compiler-cli": "^12.2.5",
    "@angular/language-service": "^12.2.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.15.3",
    "codelyzer": "^6.0.0",
    "electron": "^14.0.0",
    "electron-builder": "^22.4.1",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "build": {
    "appId": "com.anteestudio.registerprinter",
    "asar": true,
    "asarUnpack": [
      "app/RegisterPrinter.exe",
      "app/RegisterPrinter"
    ],
    "files": [
      "dist/electron/*",
      "dist/register-printer-frontend/**/*",
      "app/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "registerPrinter.ico"
    },
    "mac": {
      "target": "dmg"
    },
    "publish": {
      "provider": "generic",
      "url": "http://zhangyiant.tpddns.cn:9000/"
    }
  },
  "workspaces": [
    "packages/register-printer"
  ]
}
